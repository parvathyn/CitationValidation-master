
namespace DataAccess.Model.Parkmobile
{
    public class ParkMobileParameter
    {
        private int _zoneId;
        private int _spaceId;

        public ParkMobileParameter(int zoneId, int spaceId)
        {
            this._zoneId = zoneId;
            this._spaceId = spaceId;
        }

        public int ZoneId
        {
            get
            {
                return this._zoneId;
            }
        }

        public int SpaceId
        {
            get
            {
                return this._spaceId;
            }
        }
    }
}
